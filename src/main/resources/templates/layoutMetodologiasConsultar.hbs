<html><head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" type="text/css">
  <link rel="stylesheet" href="metodologiasConsultarTheme.css" type="text/css"> </head><body>
  <nav class="navbar navbar-expand-md navbar-dark bg-secondary">
    <div class="container">
      <a class="navbar-brand" href="#">Next</a>
      <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"> <span class="navbar-toggler-icon"></span> </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link" href="/empresas">Cuentas</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/indicadores">Indicadores</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link active" href="/metodologias">Metodologías</a>
                </li>
        </ul>
      </div>
    </div>
  </nav>
  <div class="py-3">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <a class="btn btn-secondary" href="/metodologias">Atrás</a>
        </div>
      </div>
    </div>
  </div>
  <div class="py-4">
    <div class="container">
      <div class="row"><div class="col-md-6"><p class="lead">Metodología seleccionada:&nbsp;</p></div><div class="col-md-6"><p class="lead">{{ nombre }}</p></div></div><div class="row"><div class="col-md-12"><p class="lead text-success">Resultados</p></div></div><div class="row">
        <div class="col-md-12">
          <table class="table">
            <thead>
              <tr>
                <th>#</th>
                <th>Empresa</th>
                
              </tr>
            </thead>
            <tbody>
            
              {{#each resultadoOk}}
              <tr>
                <td>{{ @index }}</td>
                <td>{{ this.nombreEmpresa }}</td>
              </tr>
              {{/each}}
              
            </tbody>
          </table>
        </div>
      </div><div class="row"><div class="col-md-12"><p class="lead text-danger">Empresas que fallaron</p></div></div><div class="row">
        <div class="col-md-12">
          <table class="table">
            <thead>
              <tr>
                <th>#</th>
                <th>Empresa</th>
                <th>Observación</th>
              </tr>
            </thead>
            <tbody>
              <tr>
              
              {{#each resultadoError}}
              <tr>
                <td>{{ @index }}</td>
                <td>{{ this.nombreEmpresa }}</td>
                <td>{{ this.observacion }}</td>
              </tr>
              {{/each}}
              
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  
  <div class="py-5 bg-dark text-white">
    <div class="container">
      <div class="row">
        <div class="col-md-12 mt-3 text-center" style="">
          <p>© Copyright 2017 GRUPO20 - All rights reserved.</p>
        </div>
      </div>
    </div>
  </div>
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>


</body></html>